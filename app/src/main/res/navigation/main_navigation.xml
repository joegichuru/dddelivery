<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:startDestination="@+id/home"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation">
    <fragment
        android:id="@+id/home"
        android:name="com.joe.dialdelivery.ui.fragments.HomeFragment"
        android:label="@string/home"

        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/browse"
        android:name="com.joe.dialdelivery.ui.fragments.BrowseFragment"
        android:label="@string/browse"
        tools:layout="@layout/fragment_browse" />

    <fragment
        android:id="@+id/orders"
        android:name="com.joe.dialdelivery.ui.fragments.OrdersFragment"
        android:label="@string/orders"
        tools:layout="@layout/fragment_orders" />

    <fragment
        android:id="@+id/account"
        android:name="com.joe.dialdelivery.ui.fragments.AccountFragment"
        android:label="@string/account"
        tools:layout="@layout/fragment_account" />
</navigation>